package duck.app;

public class DecoyDuck {
	/*****
	 * Lab 1 - Duck App
	 * @author Hannah Mittman
	 */
	
	private static int i = 1;
	private static String nameSuffix = "Decoy Duck";
	private String name;
	
	DecoyDuck() {
		this.name= nameSuffix +i;
		i++;
	}
	
	
	public String swim(){
		return "I am " + name +" of the DecoyDuck class of ducks and I am swimming \n";
	}
	
	public String toString(){
		return "I am a DecoyDuck. My name is " + name +".\n";
	}
	
}
